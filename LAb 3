  //1 part 5 task
            int L = 0;

            int[] str = new int[5];
            Console.WriteLine("Введите числа");
            for (int i = 0; i < str.Length; i++)
            {
                Console.Write($"Число с индексом {i} :");
                str[i] = int.Parse(Console.ReadLine());


            }

            for (int i = 0; i < str.Length; i++)
            {


                L += (int)Math.Pow(str[i], 2);


            }
            Console.WriteLine(Math.Sqrt(L));
            Console.ReadLine();
            
            
            
            //1 part task 10
            int[] a = new int[10];
            Console.Write(" введите индекс P: ");
            int p = int.Parse(Console.ReadLine());
            Console.Write("введите индекс Q: ");
            int q = int.Parse(Console.ReadLine());
            Console.Write("Введите число P");
            int P = int.Parse(Console.ReadLine());
            Console.Write("Введите число Q");
            int Q = int.Parse(Console.ReadLine());
            int count = 0;
            if (q > 10 && q < 0 && p < 0 && p > 10 && P > Q && p > q )
            {
                Console.WriteLine("Введено неверно");
            }
            else
            {
                for (int i = 0; i < a.Length; i++)
                {
                    a[i] = int.Parse(Console.ReadLine());
                }
                a[p] = P;
                a[q] = Q;

                for (int i = p; i < q; i++)
                {
                    count++;
                }
            }
            Console.WriteLine(count);
            Console.ReadLine();
            
             //1 часть 11 номер
            int[] a = new int[10];
            int count = 0;
            int c = 0;
            for (int i = 0; i < a.Length; i++)
            {
                a[i] = int.Parse(Console.ReadLine());
            }
            for (int i = 0; i < a.Length; i++)
            {
                if (a[i] > 0)
                {
                    count++;
                }
            }
            int[] b = new int[count];
            for (int i = 0; i < a.Length; i++)
            {
                if (a[i] < 0)
                {
                    b[c] = a[i];
                    c++;
                }
               
            }
            foreach (int x in b)
                Console.Write("{0:d} ", x);
            Console.ReadLine();
            
            
            
            //1 part task 12
            int[] a = new int[8];
            int am = 0;
            int im = 0;
            for (int i = 0; i < a.Length; i++)
            {
                a[i] = int.Parse(Console.ReadLine());
            }
            for (int i = a.Length - 1; i >= 0; i--)
            {
                if (a[i] < 0)
                {
                    am = a[i];
                    im = i;
                    break;
                }
            }
            Console.WriteLine($"Значение элемента {am}, индекс элемента  {im}");
