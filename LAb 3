             //1 part 13 task
            int[] str = new int[10];
            int[] B = new int[5];//четные
            int[] C = new int[5];//нечётные
            int c = 0;
            int b = 0;
            Console.WriteLine("Введите числа");
            for (int i = 0; i < str.Length; i++)
            {
                str[i] = int.Parse(Console.ReadLine());


            }
            for (int i = 0; i <= str.Length - 2; i += 2)
            {
                B[b] = str[i];
                b++;
            }
            for (int i = 1; i <= str.Length - 1; i += 2)
            {
                C[c] = str[i];
                c++;

            }
            foreach (int x in B)
            {
                Console.Write("{0:d} ", x);
            }
            Console.WriteLine();
            foreach (int f in C)
            {
                Console.Write("{0:d} ", f);
            }
            Console.ReadLine();
            
            
            
            // 2 PART
            
            
            
            //часть 2 задание 5
            Console.Write("Введите кол-во элементов массива str: ");
            int kol = int.Parse(Console.ReadLine());
            int[] str = new int[kol];
            int amax = str[0];
            int amin = str[0];
            int imax = 0;
            int imin = 0;
            int Count = 0;
            int index = 0;
            Console.WriteLine("Введите числа");
            for (int i = 0; i < str.Length; i++)
            {
                Console.Write($"Элемент с индексом {i} равен: ");
                str[i] = int.Parse(Console.ReadLine());


            }
            for (int i = 0; i < str.Length; i++)
            {
                if (str[i] > amax)
                {
                    amax = str[i];
                    imax = i;
                }  
               
                
            }                                        
            for (int i = 0; i < str.Length; i++)
            {
                if (str[i] < amin)
                {
                    amin = str[i];
                    imin = i;
                }
            }
            for (int i = imin + 1; i < imax; i++)
            {
                if (str[i] < 0) 
                {
                    Count++;
                }
            }
            int[] b = new int[Count];

            for (int i = imin + 1; i < imax; i++)
            {
                if (str[i] < 0)
                {
                    b[index] = str[i];
                    index += 1;
                }
            }
            for (int i = 0; i < b.Length; i++)
                Console.Write("{0:d} ", b[i]);
            Console.ReadLine();
            
            
            
            //part 2 taks 6
            Console.Write("Введите кол-во элементов массива str: ");
            int kol = int.Parse(Console.ReadLine());
            Console.Write("Введите число P, которое хотитет включить: ");
            int P = int.Parse(Console.ReadLine());
            int[] str = new int[kol];
            int sum = 0;
            int count = 0;
            var srednee = 0;
            var i_pered_srend = 0;
            for (int i = 0; i < str.Length - 1; i++)
            {
                Console.Write($"Элемент с индексом {i} равен: ");
                str[i] = int.Parse(Console.ReadLine());


            }
            foreach (int x in str)
            {
                sum += x;
                count++;
            }
            srednee = sum / count;
            for (int i = 0; i < str.Length; i++)
            {
                if (Math.Abs((str[i] - srednee)) < 1)
                {
                    i_pered_srend = i;
                }
            }
            for (int i = str.Length - 2; i > i_pered_srend; i--)
            {
                str[i + 1] = str[i];
            }
            str[i_pered_srend + 1] = P;
            for (int i = 0; i < str.Length; i++)
                Console.Write("{0:d} ", str[i]);
            Console.ReadLine();
            
            
            
             //часть 2 задание 10
            Console.Write("Введите кол-во элементов массива Array: ");
            int kol = int.Parse(Console.ReadLine());
            int[] Array = new int[kol];
            int amin = Array[0];          
            int imin = 0;         
            Console.WriteLine("Введите числа");
            for (int i = 0; i < kol; i++)
            {
                Console.Write($"Элемент с индексом {i} равен: ");
                Array[i] = int.Parse(Console.ReadLine());
                if (Array[i] < 0)
                {
                    Console.WriteLine("Число введено неверно!");
                    break;
                }


            }
            for (int i = 0; i < Array.Length; i++)
            {
                if (Array[i] < amin)
                {
                    amin = Array[i];
                    imin = i;
                }
            }
            Console.WriteLine(amin);
            for (int i = imin; i < Array.Length - 1; i++)
                Array[i] = Array[i + 1];

           
            
            for (int i = 0; i < Array.Length - 1; i++)
                Console.Write("{0:d} ", Array[i]);
            Console.ReadLine();
            
            
            //2 part task 11
             Console.Write("Введите кол-во элементов массива a: ");
            int kol = int.Parse(Console.ReadLine());
            Console.Write("Введите число P, которое хотитет включить: ");
            int P = int.Parse(Console.ReadLine());
            int[] a = new int[kol];
            int posl_element = 0;
            for (int i = 0; i < a.Length - 1; i++)
            {
                Console.Write($"Элемент с индексом {i} равен: ");
                a[i] = int.Parse(Console.ReadLine());


            }
            for (int i = kol - 1; i >= 0; i--)
            {
                if (a[i] > 0)
                {
                    posl_element = i;
                    break;
                }
            }
            for (int i = kol - 2; i > posl_element; i--)
            {
                a[i + 1] = a[i]; 
            }
            a[posl_element + 1] = P;
            foreach (int x in a)
                Console.Write("{0:d} ", x);
            Console.ReadLine();
            
            
            //2 part task 13
             int kol_vo = int.Parse(Console.ReadLine());
            int[] a = new int[kol_vo];
           
            for (int i = 0; i < a.Length; i++)
            {
                Console.Write($"Элемент с индексом {i} равен: ");
                a[i] = int.Parse(Console.ReadLine());
            }
            int amax = a[0];
            int imax = 0;
            for (int i = 0; i < kol_vo; i+=2)
            {
                if (a[i] > amax)
                {
                    a[i] = amax;
                    imax = i;
                }
            }
            for (int i = 0; i < kol_vo ; i++)
            {
                if (i % 2 == 0)
                {
                    a[i] = imax;
                }
            }
            Console.WriteLine();
            foreach (int x in a)
                Console.Write("{0:d} ", x);
            Console.ReadLine();
           
           
           //part 2 task 15
            Console.Write($"Кол-во элементов а: ");
            int kol_vo = int.Parse(Console.ReadLine());
            int[] a = new int[kol_vo];
            
            Console.Write($"Кол-во элементов b: ");
            int kol_vo1 = int.Parse(Console.ReadLine());
            int[] b = new int[kol_vo1];
            
            Console.WriteLine();
            int count = 0;
            for (int i = 0; i < a.Length; i++)
            {
                Console.Write($"Элемент a с индексом {i} равен: ");
                a[i] = int.Parse(Console.ReadLine());
            }

            Console.WriteLine();

            for (int i = 0; i < b.Length; i++)
            {
                Console.Write($"Элемент b с индексом {i} равен: ");
                b[i] = int.Parse(Console.ReadLine());
            }
            Console.WriteLine();

            Console.Write("Введите элемент K: ");

            int k = int.Parse(Console.ReadLine());

            Console.WriteLine();

            if (k < 0 && k > (kol_vo - 1))
            {
                Console.WriteLine("Число K введено неверно!");
                

                
            }
            else
            {
                int[] C = new int[kol_vo + kol_vo1];
                for (int i = 0; i <= k; i++)
                {
                    count++;
                }
                for (int i = 0; i <= k; i++)
                {
                    C[i] = a[i];
                }
              
                for (int i = k + 1,f = 0; i < (count + kol_vo1) && (f < b.Length) ; i++,f++)
                {
                    C[i] = b[f];
                   
                }
                for (int i = count + kol_vo1,f = k + 1 ; (i < C.Length) && (f < a.Length)  ; i++,f++)
                {
                    C[i] = a[f];
                }
                Console.WriteLine();
                foreach (int x in C)
                    Console.Write("{0:d} ", x);
                Console.ReadLine();
            }
            
