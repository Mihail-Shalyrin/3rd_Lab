            //3 часть 5 номер
            int kol_vo = int.Parse(Console.ReadLine());
            int[] a = new int[kol_vo];
            int amax = a[0];
            int imax = 0;
            for (int i = 0; i < a.Length; i++)
            {
                Console.Write($"Элемент с индексом {i} равен: ");
                a[i] = int.Parse(Console.ReadLine());
            }
            for (int i = 0; i < a.Length - 2; i+=2)
            {
                amax = a[i];
                imax = i;
                for (int j = i + 2; j < a.Length; j+=2)
                {
                    if (a[j] > amax)
                    {
                        amax = a[j];
                        imax = j;
                    }
                }
                a[imax] = a[i];
                a[i] = amax;

            }
            foreach (int x in a)
                Console.Write("{0:d} ", x);
            Console.ReadLine();
            
            
            
            
             //3 часть 9 номер
            Console.Write("Кол-во элементов последовательности: ");
            int kol_Vo = int.Parse(Console.ReadLine());
            int[] posl = new int[kol_Vo];
            int istart = 0;
            int i_end = 0;
            int nach_Posl = 0;
            int len_posl = 1;
            int max_len_posl = 0;
            int istart_max = 0;
            int i_end_max = 0;
            for (int i = 0; i < posl.Length; i++)
            {
                Console.Write($"Элемент с индексом {i} равен: ");
                posl[i] = int.Parse(Console.ReadLine());
            }
            do
            {
                for (int i = 0; i < posl.Length - 1; i++)
                {

                    if (posl[i] < posl[i + 1])
                    {
                        istart = nach_Posl;
                        i_end = i;
                        len_posl += 1;
                    }
                    if (len_posl > max_len_posl)
                    {
                        max_len_posl = len_posl;
                        istart_max = istart;
                        i_end_max = i_end;

                    }
                }
                nach_Posl++;
                len_posl = 1;


            } while (nach_Posl != posl.Length);
            for (int i = istart_max; i <= i_end_max; i++)
                Console.Write("{0:d} ", posl[i]);
            Console.WriteLine();
            Console.Write($"Самая большая последовательность:  {max_len_posl} ");
            Console.ReadLine();
            
            
            
            
            
            
              // 3 часть 8 номер
            int kol = int.Parse(Console.ReadLine());
            int[] up = new int[kol];
            int temp = 0;
            for (int i = 0; i < kol; i++)
            {
                up[i] = int.Parse(Console.ReadLine());
            }
            for (int i = 0; i < up.Length - 1; i++)
            {
                for (int j = i + 1; j < up.Length; j++)
                {
                    if ((up[i] < 0) && (up[j] < 0))
                    {
                        if (up[i] < up[j])
                        {
                            temp = up[i];
                            up[i] = up[j];
                            up[j] = temp;
                        }
                    }
                }
            }
            for (int i = 0; i < 10; i++)

                Console.Write(" " + up[i]);
            Console.ReadLine();
            
            
            
            
             //3 часть 12 номер
            int[] a = new int[12];
            
            for (int i = 0; i < 12; i++)
            {
                Console.Write($"Значение элемента с индексом {i}: ");
                a[i] = int.Parse(Console.ReadLine());
            }
            int amax = a[0];
            for (int i = 0; i < a.Length; i++)
            {
                if (a[i] > amax)
                {
                    a[i] = amax;
                }
            }
            for (int i = 0; i < a.Length; i++)
            {
                if (a[i] < 0)
                {
                    amax += 1;
                    a[i] = amax;
                  
                }
            }
            foreach (int x in a)
                Console.Write("{0:d} ", x);
            Console.ReadLine();
            
            
            
            
            
             //3 часть 13 номер
            int n = int.Parse(Console.ReadLine());
            int[] a = new int[n];

            for (int i = 0; i < n; i++)
            {
                Console.Write($"Значение элемента с индексом {i}: ");
                a[i] = int.Parse(Console.ReadLine());
            }
            int amax = a[0];
            for (int i = 0; i < a.Length; i++)
            {
                if (a[i] > amax)
                {
                    a[i] = amax;
                }
            }
            for (int i = 0; i < a.Length; i++)
            {
                for (int j = i + 1; j < a.Length; j++)
                {
                    if (a[i] == a[j])
                    {
                        amax += 1;
                        a[j] = amax;
                    }
                }
            }
            foreach (int x in a)
                Console.Write("{0:d} ", x);
            Console.ReadLine();
        
